{
	"name": "atomjoy/trans",
	"description": "Laravel database translations.",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"classmap": [
			"src/"
		],
		"psr-4": {
			"Trans\\": "src/",
			"Database\\Migrations\\": "database/migrations/"
		},
		"files": [
			"src/Helper.php"
		]
